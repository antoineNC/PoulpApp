{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./src",
    "paths": {
      "@firebase/auth": [
        "../node_modules/@firebase/auth/dist/index.rn.d.ts"
      ],
      "@screens/*": [
        "screens/*"
      ],
      "@context/*": [
        "context/*"
      ],
      "@navigation/*": [
        "navigation/*"
      ],
      "@styledComponents": [
        "styles/styledComponents.tsx"
      ],
      "@styles": [
        "styles/styles.ts"
      ],
      "@theme": [
        "styles/theme.ts"
      ],
      "@types": [
        "types.d.ts",
        "navigation/navigation.types.ts",
        "firebase/firebase.types.d.ts"
      ]
    },
  }
}